CryGraphEditor--CNodeStyle {
	/* Content: Background */
	qproperty-backgroundColor: rgba(35, 35, 35, 200);

	/* Content: Margins */
	qproperty-contentMarginTop: 3;
	qproperty-contentMarginLeft: 7;
	qproperty-contentMarginBottom: 6;
	qproperty-contentMarginRight: 6;

	/* Content: Rect adjustment for drawing */
	qproperty-contentRectAdjustmentTop: 0;
	qproperty-contentRectAdjustmentLeft: 0;
	qproperty-contentRectAdjustmentBottom: 0;
	qproperty-contentRectAdjustmentRight: 0;

	/* Icon */
	qproperty-pinIconSize: 9px 9px;
	qproperty-pinIconOffsetX: 0;
	qproperty-pinIconOffsetY: 0;
	qproperty-pinDefaultColor: rgb(110, 110, 110);

	/* Border */
	qproperty-borderColor: rgb(84, 84, 84);
	qproperty-borderWidth: 1;

	/* Border: Rect adjustment for drawing */
	qproperty-borderRectAdjustmentTop: 0;
	qproperty-borderRectAdjustmentLeft: 0;
	qproperty-borderRectAdjustmentBottom: 0;
	qproperty-borderRectAdjustmentRight: 0;

	/* Header */
	qproperty-headerHeight: 24.0;
	qproperty-headerIconSize: 16px 16px;
	qproperty-headerDefaultColorLeft: rgb(26, 26, 26);
	qproperty-headerDefaultColorRight: rgb(26, 26, 26);

	/* Header: Content margins */
	qproperty-headerContentMarginTop: 1;
	qproperty-headerContentMarginLeft: 7;
	qproperty-headerContentMarginBottom: 0;
	qproperty-headerContentMarginRight: -6;

	/* Header: Rect adjustment for drawing */
	qproperty-headerRectAdjustmentTop: 0;
	qproperty-headerRectAdjustmentLeft: 0;
	qproperty-headerRectAdjustmentBottom: 0;
	qproperty-headerRectAdjustmentRight: 0;
}

