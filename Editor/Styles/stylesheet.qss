}

QTabWidget#notificationTabs > QTabBar::tab:last{
	border-top-left-radius: 0px;
	border-top-right-radius: 2px;
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 2px;
	margin-left:0px;
}

QTabWidget#notificationTabs > QTabBar::tab:middle{
	margin-left:0px;
	border-top-left-radius: 0px;
	border-top-right-radius: 0px;
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 0px;
}

QTabWidget#notificationTabs > QTabBar::tab:hover {
	color:black;
	background-color:rgb(180,180,180);
}

QTabWidget#notificationTabs > QTabBar::tab:selected {
	color:black;
	background-color:rgb(97,172,236);
	padding-bottom:0px;
}

QTabWidget#notificationTabs::pane
{
