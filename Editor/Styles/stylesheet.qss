QWidget {
	background-color: rgb(66,66,66);
	font-family:"Arial";
	font-size: 12px;
	color:rgb(166,166,166);
}

QWidget::disabled{
	color:rgb(120,120,120);
}

QFrame {
	background-color:transparent;
	color:rgb(200,200,200);
	border: none;
}

QSeparators {
	background-color:transparent;
	color:transparent;
	border: none;
}

QComboBox {
    background-color: rgb(102,102,102);
    color: rgb(242,242,242);

    margin: 0px;
    padding: 0px;

    border-radius: 3px;

    height: 22px;
}

QComboBox:disabled {
    color:rgb(120,120,120);
    background-color:rgb(58,58,58);
}

QComboBox::drop-down {
    background-color: transparent;

    margin: 0px;
    padding: 0px;
}

QComboBox::drop-down:disabled {
    background-color: rgb(58,58,58);
}

QComboBox::item {
	color:rgb(166,166,166);
	background-color:rgb(50,50,50);
}

QComboBox::item:selected {
	color:black;
	background-color:rgb(97,172,236);
}

QComboBox::down-arrow {
    /*background-color: green;*/

    margin: 0px 4px 0px 0px;
    padding: 0px;

    width: 14px;
    height: 14px;
    image: url(icons:General/Pointer_Down_Tinted.ico);
}

QComboBox::down-arrow::disabled{
	width: 14px;
	height: 14px;
	image: url(icons:General/Pointer_Down_Tinted_disabled.ico);
}

QToolBar {
	background-color: rgb(35,35,35);
	border:0px solid rgb(78,78,78);
	padding-top:0px;
	padding-bottom:0px;
	padding-left:0px;
	padding-right:0px;
	icon-size: 24px;
}

QMainWindow > QToolBar {
	icon-size: 24px;
	min-height:35px;
}

QToolButton {
	border-radius: 0px;
	background-color: transparent;
	margin-right: 2px;
	margin-left:2px;
	icon-size:24px;
}

QToolButton::hover {
	border-radius: 2px;
	/*background-color: rgba(255,255,255,20);*/
	background: qlineargradient(y1:1,
				stop:1 rgba(255,255,255,20),
				stop:0.7 rgba(255,255,255,15));
}

QToolButton::checked {
	/*background-color: rgb(35,35,35);*/
	background: qlineargradient(y1:1,
				stop:0.3 rgba(0,0,0,80),
				stop:0 rgba(0,0,0,50));
	border-radius:2px;
	color:rgb(97,172,236);
}

QViewportHeader > QToolButton::menu-indicator {
	image: none;
}

QViewportHeader > QToolButton {
	icon-size:16px;
}

QViewportHeader > QToolButton {
	height:20px;
	color:rgb(166,166,166);
	border-radius:0px;
	margin-bottom:2px;
	margin-top:2px;
}

QToolButton::pressed {
	/*background-color:transparent;*/
	background: qlineargradient(y1:1,
				stop:0.3 rgba(0,0,0,40),
				stop:0 rgba(0,0,0,20));
	/*border:2px solid rgb(97,172,236);*/
	/*padding:-2px;*/
}

QViewportHeader > QToolButton::pressed {
	color:rgb(97,172,236);
	background: qlineargradient(y1:1,
				stop:0.3 rgba(0,0,0,20),
				stop:0 rgba(0,0,0,0));
	border-radius:0px;
}

QViewportHeader > QToolButton::checked{
	background-color:transparent;
}

/*QViewportHeader > QToolButton::hover {
	border-radius:2px;
}*/

