QToolWindowArea QTabBar {						/* Docking tabs */
	border-bottom:0px solid rgb(97,172,236);
}

QToolWindowArea > QTabBar {
	background-color:rgb(35,35,35);
}

 QToolWindowArea > QTabBar::tab {
	background-color: rgb(35,35,35);               /*inactive tabs*/
	color:rgb(166,166,166);
	margin:0px;
	border:0px;
	padding-left:6px;
	min-height:23px;
	border-radius:0px;
	padding-bottom:0px;
	border-radius:0px;
}

QToolWindowArea > QTabBar::tab:hover {
	color:rgb(166,166,166);
	/*background-color:rgb(78,78,78);*/
	background: qlineargradient(y1:1,
				stop:1 rgb(58,58,58),
				stop:0.5 rgb(45,45,45));
	border: none;
	border-top-left-radius: 0px;
	border-top-right-radius: 0px;
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 0px;
	border-top:0px solid rgb(78,78,78);
	border-left:0px solid rgb(78,78,78);
	border-right:0px solid rgb(78,78,78);
}

QToolWindowArea > QTabBar::tab:first, QToolWindowArea > QTabBar::tab:middle, QToolWindowArea > QTabBar::tab:last{
	border-radius:0px;
	margin:0px;
	padding-bottom:0px;

}

QToolWindowArea > QTabBar::tab:selected {
	border-bottom-color: black;
	/*background-color: rgb(66,66,66);*/
	background: qlineargradient(y1:1,
				stop:1 rgb(70,70,70),
				stop:0.1 rgb(66,66,66));
	color: rgb(200,200,200);
	border: none;
	border-top-left-radius: 0px;
	border-top-right-radius: 0px;
	border-bottom:0px solid rgb(95,95,95);
	border-top:2px solid rgb(97,172,236);
	padding-top:-2px;
}

