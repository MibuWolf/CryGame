	margin-top: 2px;
}

QTabWidget#notificationTabs::pane QLineEdit
{
	color: rgb(166,166,166);
}

QTabWidget > QTabBar > QToolButton
{
	background-color: rgb(66,66,66);
	border: 0px;
	border-radius: 0px;
	margin-bottom: 2px;
}

QTabWidget > QTabBar > QToolButton:hover
{
