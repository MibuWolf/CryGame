def build(bld):	
	
	bld.CryPlugin(
		target      = 'VehicleEditorPlugin',
		vs_filter	= 'Sandbox/Plugins',
		file_list   = 'VehicleEditorPlugin.waf_files',
		pch			= 'StdAfx.cpp',
		use			= [ 'MFCToolsPlugin' ],
		includes	= [ Path('Code/Sandbox/Plugins/MFCToolsPlugin')	]
	)